package java_useful;

public class operator {
	public static void main(String args[]) {
		int x = 9;
		int y =15;
		int z = 12;
		int a = 8;
		int b = 16;
		int c = 3;
		x += 5;
		y /=5;
		z %=5;
		a &=10;
		b |= 10;
		c ^= 8;
		
		
		System.out.println(x);
		System.out.println(y);
		System.out.println(z);
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
	}
}
