package java_useful;

public class TypeCating {
	public static void main(String args[]) {
		int myint = 28;
		double mydouble = 27.9;
		
		System.out.println(myint);
		System.out.println(mydouble);
		
		double mydd = 9.78;
		int myii = (int) mydd;
		
		System.out.println(mydd);
		System.out.println(myii);
		
		
	}
}
