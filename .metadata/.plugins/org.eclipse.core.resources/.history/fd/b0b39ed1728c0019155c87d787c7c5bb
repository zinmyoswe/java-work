package java_useful;

public class variable {
	public static void main(String args[]) {
		String name = "Thura Aung";
		System.out.println(name);
				
		int age = 17;
		System.out.println(name +"'s age is" +age);
		
		int mygirl;
		mygirl = 4;
		System.out.println(mygirl);
		
		String myname = "Thura lay";
		System.out.println("Hello "+ myname);
		
		int mynum = 14;
		float myfloat = 5.88f;
		String mytext = "Hello World";
		Boolean mybool = true;
		char myletter = 'D';
		
		System.out.println(mytext + mynum + " and "+myfloat + mybool +myletter);
		
		String firstname = "May ";
		String lastname = "Myat Noe";
		String fullname = firstname + lastname;
		
		System.out.println(fullname);
		
		int x = 4 ;
		int y = 3;
		int z = 2;
		int a = 5;
		int b = (a+z)*x;
		int c = b + y;
		
		
		
	}
}	
