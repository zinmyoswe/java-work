package java_useful;

public class operator_test {
	public static void main(String args[]) {
		int x = 3;
		int y = 3;
		int z = x+y;
		int a = 6;
		int b = 7;
		x += 4;
		y -= 4;
		a *= 5;
		b /= 3;
		
		System.out.println(x);
		System.out.println(y);
		
		System.out.println(z);
		System.out.println("-----------------");
		System.out.println(z);
	}

}
