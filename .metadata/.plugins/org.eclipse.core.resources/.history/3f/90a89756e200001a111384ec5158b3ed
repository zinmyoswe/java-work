package java_useful;

public class operator3 {
	public static void main(String args[]) {
		int x = 10;
		int y = 5;
		
		System.out.println(x>y);
		System.out.println(x<y);
		System.out.println(x>=y);
		System.out.println(x<=y);
		System.out.println(x>3 && x<10);
		System.out.println(x>3 || x<10);
		System.out.println(x>3 && x<10);
		
	}
}
